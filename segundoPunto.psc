Algoritmo TallerNumero1
	
	Definir nombres, apellidos Como Caracter;
	Definir edad Como Entero;
	Definir estatura Como Real;
	Escribir "Digite sus nombres";
	Leer nombre;
	Escribir "Digite sus apellidos";
	Leer apellidos;
	Escribir  "Digite su edad";
	Leer  edad;
	Escribir "Digite su estatura";
	Leer estatura;
	
	Imprimir "Su nombre es: " + nombre + " " + apellidos;
	Imprimir  "Su edad es :" , edad;
	Imprimir  "Su estatura es: " , estatura;
	

	
	
FinAlgoritmo
